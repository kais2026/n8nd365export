{
  "name": "Export Dynamics 365 to Excel",
  "nodes": [
    {
      "parameters": {
        "triggerTimes": [
          {
            "hour": 18,
            "minutes": 0
          }
        ]
      },
      "name": "Cron",
      "type": "n8n-nodes-base.cron",
      "typeVersion": 1,
      "position": [
        250,
        300
      ]
    },
    {
      "parameters": {
        "method": "GET",
        "url": "https://yourorg.crm.dynamics.com/api/data/v9.1/accounts",
        "authentication": "oAuth2"
      },
      "name": "HTTP Request (D365)",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 2,
      "position": [
        500,
        300
      ],
      "credentials": {
        "oAuth2Api": "Dynamics365 OAuth2"
      }
    },
    {
      "parameters": {
        "operation": "fromJson",
        "options": {
          "fileFormat": "xlsx"
        }
      },
      "name": "Spreadsheet File",
      "type": "n8n-nodes-base.spreadsheetFile",
      "typeVersion": 1,
      "position": [
        750,
        300
      ]
    },
    {
      "parameters": {
        "fileName": "Export_D365_{{$now.format(\"YYYYMMDD\")}}.xlsx",
        "dataPropertyName": "data"
      },
      "name": "Write File",
      "type": "n8n-nodes-base.writeBinaryFile",
      "typeVersion": 1,
      "position": [
        1000,
        300
      ]
    }
  ],
  "connections": {
    "Cron": {
      "main": [
        [
          {
            "node": "HTTP Request (D365)",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "HTTP Request (D365)": {
      "main": [
        [
          {
            "node": "Spreadsheet File",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Spreadsheet File": {
      "main": [
        [
          {
            "node": "Write File",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  }
}